#!/usr/bin/env node

const pets = require("../lib/pets.js");

const nopt = require("nopt");
const parsed = nopt({}, {}, process.argv, 2);

const cmd = parsed.argv.remain.shift();

(async () => {
    try {
        await pets.load()

        //pets.cli[cmd].apply(null, parsed.argv.remain);
    } catch (error) {
        console.error(error);
    }
})();
